<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <Nullable>enable</Nullable>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageId>Adhan</PackageId>
    <Version>0.9.0</Version>
    <Authors>davidpet86, stankovski</Authors>
    <Description>Muslim prayer times and qibla direction library. The package is built from a fork of Adhan-csharp project by David Peterson.</Description>
    <RepositoryUrl>https://github.com/stankovski/adhan-csharp</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
  </PropertyGroup>
</Project>